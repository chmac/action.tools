{
  "name": "action.tools-cli",
  "private": true,
  "version": "0.1.0",
  "description": "action.tools CLI",
  "keywords": [
    "typescript",
    "cli",
    "starter"
  ],
  "license": "AGPL-3.0-or-later",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "yarn run clean && tsc",
    "test": "yarn run build && ava --verbose",
    "bundle": "yarn run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "pkg": "^4.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "env-cmd": "^10.0.1",
    "mdast-util-to-string": "^1.0.7",
    "remark-parse": "^7.0.2",
    "remark-stringify": "^7.0.4",
    "remeda": "^0.0.14",
    "unified": "^8.4.2",
    "unist-builder": "^2.0.2",
    "unist-util-filter": "^2.0.1",
    "unist-util-map": "^2.0.0",
    "unist-util-modify-children": "^1.1.5",
    "unist-util-select": "^3.0.0",
    "unist-util-visit": "^2.0.1"
  }
}
